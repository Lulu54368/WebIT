
      <table>
        <tr>
          <td>Date</td>
          <td>Blood glucose level(mmol/L)</td>
          <td>Weight(kg)</td>
          <td>Doses of insulin intake(doses)</td>
          <td>Exercise Step Count(step count)</td>
        </tr>
   
      <div class="historyData">    
        {{#each history}}
        <tr>
            <td>{{this.date}}</td>
            <td>{{this.blood_level.data}}</td>
            <td>{{this.weight.data}}</td>
            <td>{{this.insulin_intake.data}}</td>
            <td>{{this.exercise.data}}</td>
        </tr>
        {{/each}}
      </div>
      </table>
      <br/><br/>